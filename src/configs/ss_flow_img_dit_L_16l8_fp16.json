{
    "models": {
        "denoiser": {
            "name": "SparseStructureFlowModel",
            "args": {
                "resolution": 16,
                "in_channels": 8,
                "out_channels": 8,
                "model_channels": 1024,
                "cond_channels": 1024,
                "num_blocks": 24,
                "num_heads": 16,
                "mlp_ratio": 4,
                "patch_size": 1,
                "pe_mode": "ape",
                "qk_rms_norm": true,
                "use_fp16": true
            }
        },
        "teacher": {
            "name": "SparseStructureFlowModel",
            "args": {
                "resolution": 16,
                "in_channels": 8,
                "out_channels": 8,
                "model_channels": 1024,
                "cond_channels": 1024,
                "num_blocks": 24,
                "num_heads": 16,
                "mlp_ratio": 4,
                "patch_size": 1,
                "pe_mode": "ape",
                "qk_rms_norm": true,
                "use_fp16": true
            }
        }
    },
    "dataset": {
        "name": "ImageConditionedSparseStructureLatent",
        "args": {
            "latent_model": "ss_enc_conv3d_16l8_fp16",
            "min_aesthetic_score": 4.5,
            "image_size": 518,
            "pretrained_ss_dec": "JeffreyXiang/TRELLIS-image-large/ckpts/ss_dec_conv3d_16l8_fp16"
        }
    },
    "trainer": {
        "name": "ImageConditionedFlowMDTCFGTrainer",
        "args": {
            "finetune_ckpt":{
                "denoiser": "JeffreyXiang/TRELLIS-image-large/ckpts/ss_flow_img_dit_L_16l8_fp16",
                "teacher": "JeffreyXiang/TRELLIS-image-large/ckpts/ss_flow_img_dit_L_16l8_fp16"
            },
            "max_steps": 1000000,
            "batch_size_per_gpu": 8,
            "batch_split": 2,
            "optimizer": {
                "name": "AdamW",
                "args": {
                    "lr": 4e-8,
                    "weight_decay": 0.0
                }
            },
            "ema_rate": [
                0.9999
            ],
            "fp16_mode": "inflat_all",
            "fp16_scale_growth": 0.001,
            "grad_clip": {
                "name": "AdaptiveGradClipper",
                "args": {
                    "max_norm": 1.0,
                    "clip_percentile": 95
                }
            },
            "i_log": 500,
            "i_sample": 10000,
            "i_save": 10000,
            "p_uncond": 0.0,
            "t_schedule": {
                "name": "logitNormal",
                "args": {
                    "mean": 1.0,
                    "std": 1.0
                }
            },
            "sigma_min": 1e-5,
            "distill_type": "discrete",
            "distill_interval": 0.01,
            "cfg_teacher_vm": 40.0,
            "cfg_teacher_vd": 100.0,
            "rescale_t": 1.0,
            "multiphase": 1,
            "lambda_vm": 1.0,
            "lambda_vd": 1.0,
            "d_norm": true,
            "image_cond_model": "dinov2_vitl14_reg"
        }
    }
}